{
  "name": "vanella",
  "version": "0.1.0",
  "description": "A writing and inspiration archiving application",
  "main": "dist/main.js",
  "scripts": {
    "start": "node build.js && electron .",
    "build": "node build.js && electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": "git+https://github.com/tanpero/Vanella.git",
  "author": "Camille",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/tanpero/Vanella/issues"
  },
  "homepage": "https://github.com/tanpero/Vanella#readme",
  "build": {
    "appId": "io.camille.vanella",
    "productName": "Vanella",
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "target": "dmg",
      "icon": "build/logo.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/logo.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/logo.png"
    },
    "asar": true,
    "extends": null,
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "emoji-toolkit": "^8.0.0",
    "esbuild": "^0.19.11",
    "esbuild-plugin-sass": "^1.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.2.0",
    "highlight.js": "^11.9.0",
    "html-to-pdfmake": "^2.5.2",
    "hyphenation.en-us": "^0.2.1",
    "mdast-util-toc": "^7.0.0",
    "regexp-util": "^2.0.0",
    "rehype-highlight": "^7.0.0",
    "rehype-inline": "^2.0.0",
    "rehype-mathjax": "^6.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-images": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-numbered-footnote-labels": "^1.1.0",
    "remark-toc": "^9.0.0",
    "remark-unwrap-images": "^4.0.0",
    "sass": "^1.70.0",
    "tex-linebreak": "^0.7.0",
    "typescript": "^5.3.3",
    "unicode-regex": "^4.0.0",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-children": "^3.0.0"
  },
  "dependencies": {
    "@codemirror/commands": "^6.3.3",
    "@codemirror/lang-markdown": "^6.2.4",
    "@codemirror/language": "^6.10.0",
    "@codemirror/language-data": "^6.4.0",
    "@codemirror/state": "^6.4.0",
    "@codemirror/text": "^0.19.6",
    "@codemirror/view": "^6.23.0",
    "codemirror": "^6.0.1",
    "emoji-toolkit": "^8.0.0",
    "highlight.js": "^11.9.0",
    "html-to-pdfmake": "^2.5.2",
    "hyphenation.en-us": "^0.2.1",
    "mdast-util-toc": "^7.0.0",
    "regexp-util": "^2.0.0",
    "rehype-document": "^7.0.3",
    "rehype-format": "^5.0.0",
    "rehype-highlight": "^7.0.0",
    "rehype-inline": "^2.0.0",
    "rehype-mathjax": "^6.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "rehype-toc": "^3.0.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-images": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-numbered-footnote-labels": "^1.1.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "remark-retext": "^6.0.0",
    "remark-stringify": "^11.0.0",
    "remark-toc": "^9.0.0",
    "remark-unwrap-images": "^4.0.0",
    "retext-emoji": "^9.0.0",
    "retext-english": "^5.0.0",
    "retext-indefinite-article": "^5.0.0",
    "retext-stringify": "^4.0.0",
    "tex-linebreak": "^0.7.0",
    "to-vfile": "^8.0.0",
    "unicode-regex": "^4.0.0",
    "unified": "^11.0.4",
    "unified-stream": "^3.0.0",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-children": "^3.0.0",
    "vfile-reporter": "^8.1.0"
  }
}
